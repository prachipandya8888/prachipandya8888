def minion_game(string):
    # your code goes here
    s = len(string)
    con,vow = 0,0


    for i in range(s):
        if string[i] in 'AEIOU':
           vow+=(s-i)
        else:
           con+=(s-i)
                
    if vow < con:
        print('Stuart ' + str(con))
    elif vow > con:
        print('Kevin ' + str(vow))
    else:
        print('Draw')
        
if __name__ == '__main__':
    s = input()
    minion_game(s)
